@{
    Layout = null;
}
@model IEnumerable<LoanApp.Models.User>
<html>
<head>
    <title>  Loan App </title>
</head>
<body>
    <table class="table">
         <tr><th>Имя</th><th>Возраст</th></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Phone</td>
            </tr>
        }
    </table>
</body>
</html>
